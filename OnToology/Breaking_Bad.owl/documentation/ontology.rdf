<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.semanticweb.org/mlopezcobo/breaking_bad#"
     xml:base="http://www.semanticweb.org/mlopezcobo/breaking_bad"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:swrl="http://www.w3.org/2003/11/swrl#"
     xmlns:swrlb="http://www.w3.org/2003/11/swrlb#"
     xmlns:breaking_bad="http://www.semanticweb.org/mlopezcobo/breaking_bad#">
    <owl:Ontology rdf:about="http://www.semanticweb.org/mlopezcobo/breaking_bad">
        <rdfs:comment xml:lang="es">El titulo de esta ontología es: Breaking Bad.</rdfs:comment>
        <rdfs:comment xml:lang="es">Esta ontologia ha sido creada por Miguel Ángel López Cobo.</rdfs:comment>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://www.w3.org/2000/01/rdf-schema#sameAs -->


    <owl:AnnotationProperty rdf:about="http://www.w3.org/2000/01/rdf-schema#sameAs"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://www.semanticweb.org/mlopezcobo/breaking_bad#hasCategory -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/mlopezcobo/breaking_bad#hasCategory">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/mlopezcobo/breaking_bad#Award"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/mlopezcobo/breaking_bad#Category"/>
        <rdfs:comment xml:lang="es">Esta propiedad de objeto relaciona un premio y su categoria. Esto permitirá que, dado un premio, el razonador pueda inferir su subclase correspondiente.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/mlopezcobo/breaking_bad#isAwarded -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/mlopezcobo/breaking_bad#isAwarded">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/mlopezcobo/breaking_bad#Person"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/mlopezcobo/breaking_bad#Series"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.semanticweb.org/mlopezcobo/breaking_bad#Award"/>
        <rdfs:comment xml:lang="es">Esta propiedad de objeto relaciona a un objeto que recibe un premio con el premio que recibe. Por ejemplo, Aaron Paul isAwarded GoldenGlobeBestActor2010 significaría que el acto Aaron Paul ganó el Globo de Oro a mejor actor de 2010.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/mlopezcobo/breaking_bad#isFromSeason -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/mlopezcobo/breaking_bad#isFromSeason">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/mlopezcobo/breaking_bad#Chapter"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/mlopezcobo/breaking_bad#Season"/>
        <rdfs:comment xml:lang="es">Esta es la propiedad que une los capitulos y sus temporadas. Un capitulo determinado debe pertenecer a una temporada determinada.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/mlopezcobo/breaking_bad#isInterpreted -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/mlopezcobo/breaking_bad#isInterpreted">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/mlopezcobo/breaking_bad#Character"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/mlopezcobo/breaking_bad#Actor"/>
        <rdfs:comment xml:lang="es">Esta propiedad de objeto es la que indica que actor representa a que personaje. Walter White isInterpreted BryanCranston significa que el personaje Walter White es interpretado por el actor Bryan Cranston.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://www.semanticweb.org/mlopezcobo/breaking_bad#hasAwardName -->


    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/mlopezcobo/breaking_bad#hasAwardName">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/mlopezcobo/breaking_bad#Award"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Esta propiedad indica el nombre de un premio. Por ejemplo, Globo de Oro, Emmy...</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/mlopezcobo/breaking_bad#hasGender -->


    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/mlopezcobo/breaking_bad#hasGender">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/mlopezcobo/breaking_bad#Character"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/mlopezcobo/breaking_bad#Person"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Esta propiedad indica el genero de un participante de la serie (Person) o de un personaje (Character)</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://www.semanticweb.org/mlopezcobo/breaking_bad#Actor -->


    <owl:Class rdf:about="http://www.semanticweb.org/mlopezcobo/breaking_bad#Actor">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/mlopezcobo/breaking_bad#Person"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/mlopezcobo/breaking_bad#Actress -->


    <owl:Class rdf:about="http://www.semanticweb.org/mlopezcobo/breaking_bad#Actress">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/mlopezcobo/breaking_bad#Actor"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/mlopezcobo/breaking_bad#MaleActor"/>
        <owl:disjointWith>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/mlopezcobo/breaking_bad#isAwarded"/>
                <owl:someValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://www.semanticweb.org/mlopezcobo/breaking_bad#BestActorCategoryAward"/>
                            <rdf:Description rdf:about="http://www.semanticweb.org/mlopezcobo/breaking_bad#BestActorDramaSeriesCategoryAward"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:someValuesFrom>
            </owl:Restriction>
        </owl:disjointWith>
        <rdfs:comment xml:lang="es">Esta subclase es el conjunto de actrices que participan en la serie.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/mlopezcobo/breaking_bad#Award -->


    <owl:Class rdf:about="http://www.semanticweb.org/mlopezcobo/breaking_bad#Award">
        <rdfs:comment xml:lang="es">Esta clase representa el conjunto de todos los objetos que son un premio en el ambito de programas de televisión.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/mlopezcobo/breaking_bad#AwardedPerson -->


    <owl:Class rdf:about="http://www.semanticweb.org/mlopezcobo/breaking_bad#AwardedPerson">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/mlopezcobo/breaking_bad#Person"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/mlopezcobo/breaking_bad#isAwarded"/>
                        <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/mlopezcobo/breaking_bad#Award"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/mlopezcobo/breaking_bad#Person"/>
        <rdfs:comment xml:lang="es">Este subgrupo representa a aquellos participantes en la serie que han sido galardonados con al menos un premio.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/mlopezcobo/breaking_bad#BestActorCategoryAward -->


    <owl:Class rdf:about="http://www.semanticweb.org/mlopezcobo/breaking_bad#BestActorCategoryAward">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/mlopezcobo/breaking_bad#Award"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/mlopezcobo/breaking_bad#hasCategory"/>
                        <owl:hasValue rdf:resource="http://www.semanticweb.org/mlopezcobo/breaking_bad#BestActor"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/mlopezcobo/breaking_bad#Award"/>
        <rdfs:comment xml:lang="es">Esta clase representa el subconjunto de todos los premios que son un premio al mejor actor.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/mlopezcobo/breaking_bad#BestActorDramaSeriesCategoryAward -->


    <owl:Class rdf:about="http://www.semanticweb.org/mlopezcobo/breaking_bad#BestActorDramaSeriesCategoryAward">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/mlopezcobo/breaking_bad#Award"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/mlopezcobo/breaking_bad#hasCategory"/>
                        <owl:hasValue rdf:resource="http://www.semanticweb.org/mlopezcobo/breaking_bad#BestActorDrama"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/mlopezcobo/breaking_bad#Award"/>
        <rdfs:comment xml:lang="es">Esta clase representa el subconjunto de todos los premios que son un premio al mejor actor de una serie de drama. No hay que interpretar que esto es un subconjunto de premio al mejor actor, ya que son categorias diferentes en cualquier entrega de premios.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/mlopezcobo/breaking_bad#BestActressCategoryAward -->


    <owl:Class rdf:about="http://www.semanticweb.org/mlopezcobo/breaking_bad#BestActressCategoryAward">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/mlopezcobo/breaking_bad#Award"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/mlopezcobo/breaking_bad#hasCategory"/>
                        <owl:hasValue rdf:resource="http://www.semanticweb.org/mlopezcobo/breaking_bad#BestActress"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/mlopezcobo/breaking_bad#Award"/>
        <rdfs:comment xml:lang="es">Esta clase representa el subconjunto de premios que son un premio a la mejor actriz.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/mlopezcobo/breaking_bad#BestActressDramaSeriesCategoryAward -->


    <owl:Class rdf:about="http://www.semanticweb.org/mlopezcobo/breaking_bad#BestActressDramaSeriesCategoryAward">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/mlopezcobo/breaking_bad#Award"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/mlopezcobo/breaking_bad#hasCategory"/>
                        <owl:hasValue rdf:resource="http://www.semanticweb.org/mlopezcobo/breaking_bad#BestActressDrama"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/mlopezcobo/breaking_bad#Award"/>
        <rdfs:comment xml:lang="es">Esta clase representa el subconjunto de todos los premios que son un premio a la mejor actriz de una serie de drama. No hay que interpretar que esto es un subconjunto de premio a la mejor actriz, ya que son categorias diferentes en cualquier entrega de premios.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/mlopezcobo/breaking_bad#BestDramaSeriesCategoryAward -->


    <owl:Class rdf:about="http://www.semanticweb.org/mlopezcobo/breaking_bad#BestDramaSeriesCategoryAward">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/mlopezcobo/breaking_bad#Award"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/mlopezcobo/breaking_bad#hasCategory"/>
                        <owl:hasValue rdf:resource="http://www.semanticweb.org/mlopezcobo/breaking_bad#BestDramaSeries"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/mlopezcobo/breaking_bad#Award"/>
        <rdfs:comment xml:lang="es">Esta clase representa el sunconjunto de premios que son un premio a la mejor serie de drama. No hay que interpretar que esto es una subclase de premios a la mejor serie, son categorias de premio diferentes.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/mlopezcobo/breaking_bad#BestScriptCategoryAward -->


    <owl:Class rdf:about="http://www.semanticweb.org/mlopezcobo/breaking_bad#BestScriptCategoryAward">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/mlopezcobo/breaking_bad#Award"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/mlopezcobo/breaking_bad#hasCategory"/>
                        <owl:hasValue rdf:resource="http://www.semanticweb.org/mlopezcobo/breaking_bad#BestScript"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/mlopezcobo/breaking_bad#Award"/>
        <rdfs:comment xml:lang="es">Esta clase es un subconjunto de todos los premios que son premio al mejor guion.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/mlopezcobo/breaking_bad#BestScriptDramaSeriesCategoryAward -->


    <owl:Class rdf:about="http://www.semanticweb.org/mlopezcobo/breaking_bad#BestScriptDramaSeriesCategoryAward">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/mlopezcobo/breaking_bad#Award"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/mlopezcobo/breaking_bad#hasCategory"/>
                        <owl:hasValue rdf:resource="http://www.semanticweb.org/mlopezcobo/breaking_bad#BestScriptDramaSeries"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/mlopezcobo/breaking_bad#Award"/>
        <rdfs:comment xml:lang="es">Esta clase representa al subconjunto de premios al mejor guion de una serie de drama. No hay que entender que es una subclase de premios al mejor guion, son categorias diferentes.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/mlopezcobo/breaking_bad#BestSeriesCategoryAward -->


    <owl:Class rdf:about="http://www.semanticweb.org/mlopezcobo/breaking_bad#BestSeriesCategoryAward">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/mlopezcobo/breaking_bad#Award"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/mlopezcobo/breaking_bad#hasCategory"/>
                        <owl:hasValue rdf:resource="http://www.semanticweb.org/mlopezcobo/breaking_bad#BestSeries"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/mlopezcobo/breaking_bad#Award"/>
        <rdfs:comment xml:lang="es">Esta clase es un subconjunto de todos los premios que son un premio a la mejor serie.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/mlopezcobo/breaking_bad#Category -->


    <owl:Class rdf:about="http://www.semanticweb.org/mlopezcobo/breaking_bad#Category">
        <owl:equivalentClass>
            <owl:Class>
                <owl:oneOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/mlopezcobo/breaking_bad#BestActor"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/mlopezcobo/breaking_bad#BestActorDrama"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/mlopezcobo/breaking_bad#BestActress"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/mlopezcobo/breaking_bad#BestActressDrama"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/mlopezcobo/breaking_bad#BestDramaSeries"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/mlopezcobo/breaking_bad#BestScript"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/mlopezcobo/breaking_bad#BestScriptDramaSeries"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/mlopezcobo/breaking_bad#BestSeries"/>
                </owl:oneOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:comment xml:lang="es">Esto es una subclase enumerada que representa todas las categorias posibles que puede tener un premio. Su utilidad es poder inferir la categoria de un premio mediante la propiedad de objeto hasCategory.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/mlopezcobo/breaking_bad#Chapter -->


    <owl:Class rdf:about="http://www.semanticweb.org/mlopezcobo/breaking_bad#Chapter">
        <rdfs:comment xml:lang="es">Esta clase representa el subgrupo de objetos que son el capitulo de Breaking Bad.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/mlopezcobo/breaking_bad#Character -->


    <owl:Class rdf:about="http://www.semanticweb.org/mlopezcobo/breaking_bad#Character">
        <rdfs:comment xml:lang="es">Esta clase es el subconjunto de objetos que son un personaje de la serie Breaking Bad.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/mlopezcobo/breaking_bad#Cinematography -->


    <owl:Class rdf:about="http://www.semanticweb.org/mlopezcobo/breaking_bad#Cinematography">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/mlopezcobo/breaking_bad#Person"/>
        <rdfs:comment xml:lang="es">Esta subclase representa al subconjunto de participantes que son expertos en camara y cinematografia en la serie.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/mlopezcobo/breaking_bad#Composer -->


    <owl:Class rdf:about="http://www.semanticweb.org/mlopezcobo/breaking_bad#Composer">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/mlopezcobo/breaking_bad#Person"/>
        <rdfs:comment xml:lang="es">Esta subclase representa al subconjunto de participantes que son compositores de la serie.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/mlopezcobo/breaking_bad#Director -->


    <owl:Class rdf:about="http://www.semanticweb.org/mlopezcobo/breaking_bad#Director">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/mlopezcobo/breaking_bad#Person"/>
        <rdfs:comment xml:lang="es">Esta subclase representa al subconjunto de participantes que son directores de la serie.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/mlopezcobo/breaking_bad#Editor -->


    <owl:Class rdf:about="http://www.semanticweb.org/mlopezcobo/breaking_bad#Editor">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/mlopezcobo/breaking_bad#Person"/>
        <rdfs:comment xml:lang="es">Esta subclase representa al subconjunto de participantes que son editores de la serie.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/mlopezcobo/breaking_bad#EmmyAward -->


    <owl:Class rdf:about="http://www.semanticweb.org/mlopezcobo/breaking_bad#EmmyAward">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/mlopezcobo/breaking_bad#Award"/>
        <rdfs:comment xml:lang="es">Este subconjunto de premios son todos aquellos que son un Emmy.</rdfs:comment>
        <rdfs:sameAs>https://dbpedia.org/page/Emmy_Awards</rdfs:sameAs>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/mlopezcobo/breaking_bad#GoldenGlobeAward -->


    <owl:Class rdf:about="http://www.semanticweb.org/mlopezcobo/breaking_bad#GoldenGlobeAward">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/mlopezcobo/breaking_bad#Award"/>
        <rdfs:comment xml:lang="es">Este subconjunto de premios son todos aquellos que son un Globo de Oro.</rdfs:comment>
        <rdfs:sameAs>https://dbpedia.org/page/Golden_Globe_Awards</rdfs:sameAs>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/mlopezcobo/breaking_bad#MainCharacter -->


    <owl:Class rdf:about="http://www.semanticweb.org/mlopezcobo/breaking_bad#MainCharacter">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/mlopezcobo/breaking_bad#Character"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/mlopezcobo/breaking_bad#SecondaryCharacter"/>
        <rdfs:comment xml:lang="es">Esta subclase es un conjunto de aquellos personajes que son personajes principales de la serie Breaking Bad.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/mlopezcobo/breaking_bad#MaleActor -->


    <owl:Class rdf:about="http://www.semanticweb.org/mlopezcobo/breaking_bad#MaleActor">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/mlopezcobo/breaking_bad#Actor"/>
        <owl:disjointWith>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/mlopezcobo/breaking_bad#isAwarded"/>
                <owl:someValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
